# include "header.h"

inta main()
{
inta j;
inta main_a[8]={1,2,3,4,5,6,7,8};
inta main_b[8]={1,2,3,4,5,6,7,8};
inta main_c[8];

simple(main_a,main_b,main_c);
for(j=0; j<8; j++)
{
printf("%d\n", main_c[j]);
}

} 

