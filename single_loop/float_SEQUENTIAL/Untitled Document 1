#endif


//ADD_T.c//

# include <stdio.h>
# include "header.h"

 int  main()
{
int j;
float main_a[8]={1,2,3,4,5,6,7,8};
float main_b[8]={1,2,3,4,5,6,7,8};
float main_c[8];
simple(main_a,main_b,main_c);
for(j=0; j<1; j++)
{
printf("%f\n", main_c[j]);
}
return 0;
}



